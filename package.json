{
  "name": "BreakdanceDB",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "ok": "yarn format:fix && yarn ts && yarn lint",
    "dev:ts": "yarn dev & yarn ts:watch",
    "ts": "tsc --noEmit --incremental",
    "ts:watch": "yarn ts --watch",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore .",
    "precommit": "lint-staged --allow-empty",
    "postinstall": "husky"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ],
    "*.tsx": "prettier --write"
  },
  "dependencies": {
    "@auth/mongodb-adapter": "^3.3.0",
    "@tanstack/react-query": "^5.49.2",
    "immer": "^10.1.1",
    "mongodb": "^6.7.0",
    "next": "^14.1.2",
    "next-auth": "^5.0.0-beta.19",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.51.4",
    "uuid": "^9.0.1",
    "zod": "^3.23.8",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@redux-devtools/extension": "^3.3.0",
    "@tanstack/eslint-plugin-query": "^5.49.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.8",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.4",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "sass": "^1.77.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
